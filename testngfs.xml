      <class name="com.flipsnap.test.Flipkart"/>
    </classes>
  </test> <!-- FirefoxTest -->
   <test name="EdgeTest">
  <parameter name="browser" value="edge"></parameter>
    <classes>
      <class name="com.flipsnap.test.Flipkart"/>
    </classes>
  </test> <!-- EdgeTest -->
</suite> <!-- FlipSnapSuite -->
